<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:jaxws="http://cxf.apache.org/jaxws"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd">
	<import resource="classpath:META-INF/cxf/cxf.xml" />
	<import resource="classpath:META-INF/cxf/cxf-extension-soap.xml" />
	<import resource="classpath:META-INF/cxf/cxf-servlet.xml" />
	<jaxws:endpoint xmlns:tns="http://www.apromore.org/data_access/service_canoniser"
		id="dacanoniserporttype"
		implementor="org.apromore.data_access.service_canoniser.DACanoniserPortTypeImpl"
		wsdlLocation="wsdl/da_canoniser.wsdl" endpointName="tns:DACanoniser"
		serviceName="tns:DACanoniserService" address="/DACanoniser">
		<jaxws:features>
			<bean class="org.apache.cxf.feature.LoggingFeature" />
		</jaxws:features>
	</jaxws:endpoint>
	<jaxws:endpoint xmlns:tns="http://www.apromore.org/data_access/service_manager"
		id="damanagerporttype" implementor="org.apromore.data_access.service_manager.DAManagerPortTypeImpl"
		wsdlLocation="wsdl/da_manager.wsdl" endpointName="tns:DAManager"
		serviceName="tns:DAManagerService" address="/DAManager">
		<jaxws:features>
			<bean class="org.apache.cxf.feature.LoggingFeature" />
		</jaxws:features>
	</jaxws:endpoint>
	<jaxws:endpoint xmlns:tns="http://www.apromore.org/data_access/service_toolbox"
		id="datoolboxporttype" implementor="org.apromore.data_access.service_toolbox.DAToolboxPortTypeImpl"
		wsdlLocation="wsdl/da_toolbox.wsdl" endpointName="tns:DAToolbox"
		serviceName="tns:DAToolboxService" address="/DAToolbox">
		<jaxws:features>
			<bean class="org.apache.cxf.feature.LoggingFeature" />
		</jaxws:features>
	</jaxws:endpoint>
</beans>
