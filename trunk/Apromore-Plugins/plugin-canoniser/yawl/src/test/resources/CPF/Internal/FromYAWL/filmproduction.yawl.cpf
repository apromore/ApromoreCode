            <inputExpr>
                <language>http://www.w3.org/2005/xpath-functions/</language>
                <expression>DPRinfo = &lt;DPRinfo&gt;{/Film_Production_Process/shootingSchedule/startDate} {/Film_Production_Process/shootingSchedule/scheduledFinish} {/Film_Production_Process/shootingSchedule/revisedFinish}  &lt;shootingDaysSchedule&gt;   &lt;scheduledDays&gt;     {/Film_Production_Process/shootingSchedule/scheduledShootingDays/text()}   &lt;/scheduledDays&gt;   &lt;daysToDate&gt;     {/Film_Production_Process/shootDayNoToday/text()}   &lt;/daysToDate&gt;   &lt;estdToComplete&gt;     {number(/Film_Production_Process/shootingSchedule/scheduledShootingDays/text())-     number(/Film_Production_Process/shootDayNoToday/text())}   &lt;/estdToComplete&gt;   &lt;estdTotal&gt;     {/Film_Production_Process/shootingSchedule/scheduledShootingDays/text()}   &lt;/estdTotal&gt; &lt;/shootingDaysSchedule&gt;  {/Film_Production_Process/continuityDailyReport/locationSets}  &lt;slateNOs&gt;   {/Film_Production_Process/continuityDailyReport/slateNos/singleUnit/slate} &lt;/slateNOs&gt;  {/Film_Production_Process/continuityDailyReport/scheduledScenesShot} {/Film_Production_Process/continuityDailyReport/scheduledScenesNotShot} &lt;ScenesNotYetCompleted&gt;{/Film_Production_Process/continuityDailyReport/scenesNotYetCompleted/*}&lt;/ScenesNotYetCompleted&gt; &lt;ScenesDeleted&gt;{/Film_Production_Process/continuityDailyReport/scenesDeleted/*}&lt;/ScenesDeleted&gt; &lt;ScenesAdded&gt;{/Film_Production_Process/continuityDailyReport/scenesAdded/*}&lt;/ScenesAdded&gt; {/Film_Production_Process/continuityDailyReport/unscheduledScenesShot}  {/Film_Production_Process/continuityDailyReport/scriptTiming}  &lt;ratioTimingSpec&gt;   {/Film_Production_Process/scheduledRatio}   &lt;dailyRatio&gt;     {let $x:= /Film_Production_Process/continuityDailyReport/scriptTiming/shotToday/actualTiming,     $a:= number(substring($x,1,2))*60+number(substring($x,4,2))+(round(number(substring($x,7,2))*100 div 60) div 100),     $b:= /Film_Production_Process/stockInfoToday/gross     return (round($b*100 div ($a*36)) div 100)}   &lt;/dailyRatio&gt;   &lt;averageRatio&gt;     {let $x:= /Film_Production_Process/continuityDailyReport/scriptTiming/shotToDate/actualTiming,     $a:= number(substring($x,1,2))*60+number(substring($x,4,2))+(round(number(substring($x,7,2))*100 div 60) div 100),     $b:= /Film_Production_Process/stockInfoPrevious/gross,     $c:= /Film_Production_Process/stockInfoToday/gross     return (round(($b+$c)*100 div ($a*36)) div 100)}   &lt;/averageRatio&gt;   &lt;averageTiming&gt;     {let $x:= /Film_Production_Process/continuityDailyReport/scriptTiming/shotToDate/actualTiming,     $y:= /Film_Production_Process/shootDayNoToday,     $z:= round((number(substring($x,1,2))*3600 + number(substring($x,4,2))*60 + number(substring($x,7,2))) div $y),     $s:=($z mod 3600) mod 60,     $m:=(($z - $s) div 60) mod 60,     $h:= round((($z - $s) div 60 - $m) div 60),     $S:= if ($s &gt; 10) then string($s) else (if ($s &gt; 0) then concat('0',string($s)) else '00'),     $M:= if ($m &gt; 10) then string($m) else (if ($m &gt; 0) then concat('0',string($m)) else '00'),     $H:= if ($h &gt; 10) then string($h) else (if ($h &gt; 0) then concat('0',string($h)) else '00')     return concat(string($H),':',string($M),':',string($S))}   &lt;/averageTiming&gt;   {let $x:= /Film_Production_Process/continuityDailyReport/scriptTiming/shotToDate/actualTiming,   $y:= /Film_Production_Process/continuityDailyReport/scriptTiming/shotToDate/estTiming,   $u:= number(substring($x,1,2))*3600 + number(substring($x,4,2))*60 + number(substring($x,7,2)),   $v:= number(substring($y,1,2))*3600 + number(substring($y,4,2))*60 + number(substring($y,7,2)),   $z:= abs($u - $v),   $s:= ($z mod 3600) mod 60,   $m:= (($z - $s) div 60) mod 60,   $h:= round((($z - $s) div 60 - $m) div 60),   $S:= if ($s &gt; 10) then string($s) else (if ($s &gt; 0) then concat('0',string($s)) else '00'),   $M:= if ($m &gt; 10) then string($m) else (if ($m &gt; 0) then concat('0',string($m)) else '00'),   $H:= if ($h &gt; 10) then string($h) else (if ($h &gt; 0) then concat('0',string($h)) else '00')   return   &lt;cumulative&gt;     &lt;sign&gt;       {if ($u &gt;= $v) then true() else false()}     &lt;/sign&gt;     &lt;varTime&gt;       {concat(string($H),':',string($M),':',string($S))}     &lt;/varTime&gt;   &lt;/cumulative&gt;}   {/Film_Production_Process/originalTiming} &lt;/ratioTimingSpec&gt;  {for $x in /Film_Production_Process/stockInfoPrevious,  $y in /Film_Production_Process/stockInfoToday return &lt;stockInfo&gt;     &lt;previously&gt;       {$x/*}       &lt;soundRolls&gt;{/Film_Production_Process/soundRollsPrevious/text()}&lt;/soundRolls&gt;       &lt;camRolls&gt;{/Film_Production_Process/camRollsPrevious/text()}&lt;/camRolls&gt;     &lt;/previously&gt;     &lt;today&gt;       {$y/*}       &lt;soundRolls&gt;{/Film_Production_Process/soundRollsToday/text()}&lt;/soundRolls&gt;       &lt;camRolls&gt;{/Film_Production_Process/camRollsToday/text()}&lt;/camRolls&gt;     &lt;/today&gt;     &lt;totalToDate&gt;       &lt;loaded&gt;{number($x/loaded/text())+number($y/loaded/text())}&lt;/loaded&gt;       &lt;gross&gt;{number($x/gross/text())+number($y/gross/text())}&lt;/gross&gt;       &lt;exposed&gt;{number($x/exposed/text())+number($y/exposed/text())}&lt;/exposed&gt;       &lt;print&gt;{number($x/print/text())+number($y/print/text())}&lt;/print&gt;       &lt;N_G&gt;{number($x/N_G/text())+number($y/N_G/text())}&lt;/N_G&gt;       &lt;waste&gt;{number($x/waste/text())+number($y/waste/text())}&lt;/waste&gt;       &lt;shortEnds&gt;{number($x/shortEnds/text())+number($y/shortEnds/text())}&lt;/shortEnds&gt;       &lt;soundRolls&gt;         {if (/Film_Production_Process/soundRollsPrevious/text()='nil')         then /Film_Production_Process/soundRollsToday/text() else         concat(/Film_Production_Process/soundRollsPrevious/text(),' ',         /Film_Production_Process/soundRollsToday/text())}       &lt;/soundRolls&gt;       &lt;camRolls&gt;         {if (/Film_Production_Process/camRollsPrevious/text()='nil')         then /Film_Production_Process/camRollsToday/text() else         concat(/Film_Production_Process/camRollsPrevious/text(),' ',         /Film_Production_Process/camRollsToday/text())}       &lt;/camRolls&gt;     &lt;/totalToDate&gt; &lt;/stockInfo&gt;}  &lt;artistTimeSheet&gt;   {for $x in /Film_Production_Process/timeSheetInfo/artistTimeSheet/singleArtist   return   &lt;singleArtist&gt;     {$x/artist}     {if (for $y in /Film_Production_Process/castInfo/singleCastInfo          where $y/artist=$x/artist return true())      then (for $y in /Film_Production_Process/castInfo/singleCastInfo          where $y/artist=$x/artist return $y/character)      else &lt;character&gt;character?&lt;/character&gt;}     {$x/P_U}     {$x/MU_WD_Call_scheduled}     {$x/MU_WD_Call_actualArrival}     {$x/mealBreak}     {$x/timeWrap}     {$x/travel}     &lt;totalHRs&gt;00:00:00&lt;/totalHRs&gt;   &lt;/singleArtist&gt;} &lt;/artistTimeSheet&gt;  &lt;extrasTimeSheet&gt;   {for $x in /Film_Production_Process/timeSheetInfo/extrasTimeSheet/singleArtist   return   &lt;singleArtist&gt;     {$x/artist}     &lt;character&gt;background character&lt;/character&gt;     {$x/P_U}     {$x/MU_WD_Call_scheduled}     {$x/MU_WD_Call_actualArrival}     {$x/mealBreak}     {$x/timeWrap}     {$x/travel}     &lt;totalHRs&gt;00:00:00&lt;/totalHRs&gt;   &lt;/singleArtist&gt;} &lt;/extrasTimeSheet&gt;  &lt;crewTimeSheet&gt;   {for $x in /Film_Production_Process/timeSheetInfo/crewTimeSheet/singleCrew   return   &lt;singleCrew&gt;     &lt;crewName&gt;       {for $y in /Film_Production_Process/crewInfo/singleCrewInfo       where $y/role=$x/crew return concat($y/firstName,' ',$y/lastName)}     &lt;/crewName&gt;     &lt;crewRole&gt;{$x/crew/text()}&lt;/crewRole&gt;     {$x/crewCall}     {$x/travelIn}     {$x/locationCall}     {if ($x/MU_WD_Call_mealBreak='00:00:00')     then &lt;mealBreak&gt;00:00:00&lt;/mealBreak&gt;     else &lt;mealBreak&gt;00:45:00&lt;/mealBreak&gt;}     {$x/wrap}     {$x/wrapLoc}     {$x/departLoc}     {$x/travelOut}     &lt;totalHRs&gt;00:00:00&lt;/totalHRs&gt;   &lt;/singleCrew&gt;} &lt;/crewTimeSheet&gt;  &lt;majorProps_actionVehicles_additionalEquipment&gt;   {/Film_Production_Process/timeSheetInfo/majorProps_actionVehicles_extraEquipment/text()} &lt;/majorProps_actionVehicles_additionalEquipment&gt;   &lt;additionalCrew&gt;   {/Film_Production_Process/timeSheetInfo/additionalPersonnel/text()} &lt;/additionalCrew&gt;   &lt;livestocks_other&gt;   {/Film_Production_Process/timeSheetInfo/livestock/text()} &lt;/livestocks_other&gt;   &lt;accidents_delays&gt;   {/Film_Production_Process/timeSheetInfo/accidents_delays/text()} &lt;/accidents_delays&gt;  &lt;catering&gt;   {for $x in /Film_Production_Process/timeSheetInfo/mealInfo/singleMeal return   &lt;singleMeal&gt;     {$x/meal}     &lt;time&gt;{$x/duration/text()}&lt;/time&gt;     {$x/numbers}     {$x/location}   &lt;/singleMeal&gt;} &lt;/catering&gt;  &lt;generalRemarks&gt;   {concat(/Film_Production_Process/timeSheetInfo/generalComments/text(),   /Film_Production_Process/continuityDailyReport/remarks/text())} &lt;/generalRemarks&gt;&lt;/DPRinfo&gt;</expression>
            </inputExpr>
            <inputExpr>
                <language>http://www.w3.org/2005/xpath-functions/</language>
                <expression>producer = &lt;producer&gt;{/Film_Production_Process/crewMember/producer/text()}&lt;/producer&gt;</expression>
            </inputExpr>
            <inputExpr>
